<header class="s-header s-header--plateforme sticky-top" role="banner" id="header">
  <section class="s-header__container container">
    <div class="s-header__row row">
      <div class="s-header__col s-header__col--logo col-auto d-flex align-items-center pr-0 pr-md-3">
        <a href="#top" class="s-header__logo-ministere"><img src="<%= typeof pathDepth != 'undefined' ? pathDepth : './..' %>/images/logo-ministere-emploi.svg" alt="Ministère du travail, du plein emploi et de l'insertion"></a>
      </div>
      <div class="s-header__col s-header__col--logo col-auto d-flex align-items-center px-0 px-md-3">
        <a href="<%= typeof pathDepth != 'undefined' ? pathDepth : './..' %>"><img src="<%= typeof pathDepth != 'undefined' ? pathDepth : './..' %>/images/logo-plateforme-inclusion.svg" alt="La Plateforme de l'inclusion"></a>
      </div>
      <div class="s-header__col s-header__col--nav col d-none d-xl-inline-flex d-xl-flex align-items-center justify-content-end">
        <nav id="nav-primary" role="navigation" aria-label="Navigation principale">
          <ul>
            <li>
              <a href="https://emplois.inclusion.beta.gouv.fr" class="is-emploi" rel="noopener" target="_blank" title="Accéder aux emplois de l'inclusion, service numérique de délivrance des PASS IAE">
                <span>Les emplois</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://communaute.inclusion.beta.gouv.fr" class="is-communaute" rel="noopener" target="_blank" title="Accéder à la communauté de l'inclusion, espace d'échanges, de rencontres et de partages entre les acteurs et actrices de l'insertion.">
                <span>La communauté</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://lemarche.inclusion.beta.gouv.fr" class="is-marche" rel="noopener" target="_blank" title="Accéder au marché de l'inclusion, un service numérique permettant d'accéder à l'annuaire des entreprises sociales inclusives.">
                <span>Le marché</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://pilotage.inclusion.beta.gouv.fr" class="is-pilotage" rel="noopener" target="_blank" title="Accéder au pilotage de l'inclusion, un service ayant pour objectif de réduire la charge administrative.">
                <span>Le pilotage</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
          </ul>
        </nav>
      </div>
      <div class="s-header__col s-header__col--burger col d-flex align-items-center justify-content-end d-xl-none" data-toggle="burger" aria-controls="burgerNav" tabindex="0">
        <span id="burger">
          <svg xmlns="http://www.w3.org/2000/svg" width="36" height="51" viewBox="0 0 36 51" role="img" aria-labelledby="title_burger">
            <g fill="none" fill-rule="evenodd">
              <g fill="#000638" fill-rule="nonzero" transform="translate(2)">
                <rect width="32" height="4" rx="2"></rect>
                <rect width="32" height="4" y="12" rx="2"></rect>
                <rect width="32" height="4" y="24" rx="2"></rect>
              </g>
              <text fill="#000638" font-family="Roboto" font-size="13" font-weight="400">
                <tspan x="2" y="45" id="title_burger">Menu</tspan>
              </text>
            </g>
          </svg>
        </span>
        <span id="close">
          <svg xmlns="http://www.w3.org/2000/svg" width="44" height="45" viewBox="0 0 44 45" role="img" aria-labelledby="title_close">
            <g fill="none" fill-rule="evenodd" transform="translate(-1 -2)">
              <g fill="#000638" fill-rule="nonzero" transform="translate(10 1)">
                <rect width="32" height="4" x="-3" y="11" rx="2" transform="rotate(45 13 13)"></rect>
                <rect width="32" height="4" x="-3" y="11" rx="2" transform="rotate(-45 13 13)"></rect>
              </g>
              <text fill="#000638" font-family="Roboto" font-size="13" font-weight="400">
                <tspan x="3" y="45" id="title_close">Fermer</tspan>
              </text>
            </g>
          </svg>
        </span>
      </div>
      <div id="burgerNav">
        <nav role="navigation">
          <ul>
            <li>
              <a href="https://emplois.inclusion.beta.gouv.fr" class="is-emploi" rel="noopener" target="_blank" title="Accéder aux emplois de l'inclusion, service numérique de délivrance des PASS IAE">
                <span>Les emplois</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://communaute.inclusion.beta.gouv.fr" class="is-communaute" rel="noopener" target="_blank" title="Accéder à la communauté de l'inclusion, espace d'échanges, de rencontres et de partages entre les acteurs et actrices de l'insertion.">
                <span>La communauté</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://lemarche.inclusion.beta.gouv.fr" class="is-marche" rel="noopener" target="_blank" title="Accéder au marché de l'inclusion, un service numérique permettant d'accéder à l'annuaire des entreprises sociales inclusives.">
                <span>Le marché</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
            <li>
              <a href="https://pilotage.inclusion.beta.gouv.fr" class="is-pilotage" rel="noopener" target="_blank" title="Accéder au pilotage de l'inclusion, un service ayant pour objectif de réduire la charge administrative.">
                <span>Le pilotage</span>
                <i class="ri-arrow-right-up-line ri-lg"></i>
              </a>
            </li>
          </ul>
        </nav>
      </div>
    </div>
  </section>
</header>
